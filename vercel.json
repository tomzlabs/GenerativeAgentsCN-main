{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/**/*.py": {
      "maxDuration": 30,
      "includeFiles": "generative_agents/**"
    }
  },
  "rewrites": [
    { "source": "/api/health", "destination": "/api/health.py" },
    { "source": "/static/(.*)", "destination": "/api/index.py" },
    { "source": "/(.*)", "destination": "/api/index.py" }
  ]
}
