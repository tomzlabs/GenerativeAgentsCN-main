{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 30,
        "includeFiles": "generative_agents/**"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/health", "dest": "/api/health.py" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ]
}
